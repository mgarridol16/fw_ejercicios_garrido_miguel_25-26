{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "rootDir": "./src/ts",
    "sourceMap": true,
    "outDir": "./public/dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/ts"],
  "exclude": ["node_modules"]
}
